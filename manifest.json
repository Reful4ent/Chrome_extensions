{
  "manifest_version": 3,
  "name": "Scroll button",
  "version": "1.0",
  "description": "Scroll down the page or scroll up as you wish",
  "icons": {
    "16": "images/start-16.png",
    "32": "images/start-32.png",
    "48": "images/start-48.png",
    "128": "images/start-128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/start-16.png",
      "32": "images/start-32.png",
      "48": "images/start-48.png",
      "128": "images/start-128.png"
    }
  },
  "permissions": ["scripting", "activeTab"],
  "background":{}
}